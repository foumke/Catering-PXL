<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CateringPXL.CardWrap">
  <ContentView.Content BackgroundColor="White">
    
    <StackLayout Padding="10, 10, 10, 10" BackgroundColor="White">
      <Label x:Name="lblTitle" FontAttributes="Bold" Text="Onbekend" TextColor="#58A518" />
      <StackLayout Padding="10, 0, 0, 0" Spacing="0">
        <Label x:Name="lblContent" FontAttributes="Bold" Font="large" Text="Onbekend" TextColor="Black"/>
      </StackLayout>
    </StackLayout>
    
  </ContentView.Content>
</ContentView>